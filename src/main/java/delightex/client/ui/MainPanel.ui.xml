<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
  <g:HTMLPanel>
    <g:VerticalPanel>
      <g:Label ui:field="helloText" height="30"/>
      <g:HorizontalPanel>
        <g:TextBox ui:field="chatName"/>
        <g:Button text="Create Chat" ui:field="newButton"/>
      </g:HorizontalPanel>
      <g:VerticalPanel ui:field="chatList"/>
    </g:VerticalPanel>
  </g:HTMLPanel>
</ui:UiBinder>